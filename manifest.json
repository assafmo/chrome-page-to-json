{
  "manifest_version": 2,
  "name": "Page to JSON",
  "description": "A Chrome extension to download a JSON file containing data structures from the current tab",
  "version": "0.2.1",
  "icons": {
    "32": "icon.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Page to JSON"
  },
  "background": {
    "scripts": [
      "jquery-3.1.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "<all_urls>"
  ]
}